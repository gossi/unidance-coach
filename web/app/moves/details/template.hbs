{{#let @model.move as |move|}}
  {{#if move}}
    {{page-title move.title}}

    <Moves::Details
      @move={{@model.move}}
      @buildMoveLink={{@model.buildMoveLink}}
      @buildSkillLink={{@model.buildSkillLink}}
      @buildGameLink={{@model.buildGameLink}}
    />
  {{else}}
    <h1>Not Found</h1>

    <p>whoops</p>
  {{/if}}
{{/let}}